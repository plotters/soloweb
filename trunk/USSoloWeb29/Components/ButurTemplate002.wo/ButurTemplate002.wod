HEADING_CONDITIONAL: WOConditional {
	condition = currentComponent.textOne.length;
}

IMAGE: SWImage {
	document = currentComponent.document;
	align = "left";
}

HEADING_STRING: WOString {
	value = currentComponent.textOne;
	escapeHTML = false;
}

TEXT: WOString {
	value = currentComponent.textTwoRendered;
	escapeHTML = false;
}